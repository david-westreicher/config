# use 256 colors, disable ctrl+s
alias ll='ls -lah --color=auto'
export TERM=xterm-256color
bind -r '\C-s'
stty -ixon -ixoff
PS1='${debian_chroot:+($debian_chroot)}\[\033[01;32m\]\u@\h\[\033[00m\]:\[\033[01;34m\]\w\[\033[00m\]\n\[\033[01;32m\]>\[\033[00m\] '
